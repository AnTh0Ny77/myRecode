{% extends 'base.html.twig' %}
    {% block style %}
   
    {% endblock %}
{% block main %}
    {% include "elements/navbar.html.twig" %}
    {% if alert %}
        <div class="alert alert-recode alert-warning alert-dismissible fade show" role="alert">
            <div class="div-alert">
                <b>
                    {{alert.message}}
                </b>
            </div>   
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    {% endif %}
        <main class=" d-flex">
            <div class="mx-auto">
                {% include "forms/client_forms.html.twig" %}
               
            </div>
         
        </main>
{% endblock %}
    {% block script %}
    <script 
        src="https://maps.googleapis.com/maps/api/js?key=Your_api_key&libraries=places">
    </script>
    <script 
        src="public/js/place.js">
    </script>

    {% endblock %}